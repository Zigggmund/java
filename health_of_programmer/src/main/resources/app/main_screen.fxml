      <MenuBar prefHeight="34.0" prefWidth="700.0">
        <menus>
          <Menu mnemonicParsing="false" onAction="#new_window" text="Правильная осанка">
            <items>
              <MenuItem mnemonicParsing="false" text="Открыть" />
